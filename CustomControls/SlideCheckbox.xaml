<UserControl x:Class="TeamProgramWithDatabase.CustomControls.SlideCheckbox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TeamProgramWithDatabase.CustomControls"
             mc:Ignorable="d" 
             d:DesignHeight="32" d:DesignWidth="62">
    <Grid>
        <Rectangle x:Name="BackgroundRect" Fill="#FFA6A6A6" HorizontalAlignment="Left" Height="30" Margin="0,0" RadiusX="15" RadiusY="15" VerticalAlignment="Top" Width="55" Cursor="Hand" ForceCursor="True"/>
        <Ellipse x:Name="Circle" Fill="#FFC80000" HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="30" Cursor="Hand" ForceCursor="True"/>

    </Grid>
</UserControl>
