<Page x:Class="TeamProgramWithDatabase.GeneralPageContent.General_Notice"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:TeamProgramWithDatabase.GeneralPageContent"
      mc:Ignorable="d" 
      d:DesignHeight="40" d:DesignWidth="923"
      Title="General_Notice">

    <Grid ClipToBounds="True">
       <Canvas x:Name="Canvass"  HorizontalAlignment="Left" Height="84" VerticalAlignment="Top" Width="923" Margin="0,0,0,-44" Grid.ColumnSpan="2">
            <Label x:Name="CurrentMessage"  Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/TeamProgramWithDatabase;component/Resources/#210 MGothicOTF 030" FontSize="28" Canvas.Left="10" Canvas.Top="44" Foreground="#4d4d4d"/>
            <Label x:Name="BackwardMessage"  Content="Label" HorizontalAlignment="Left" VerticalAlignment="Top" FontFamily="/TeamProgramWithDatabase;component/Resources/#210 MGothicOTF 030" FontSize="28" Canvas.Left="10" Canvas.Top="0" Foreground="#4d4d4d"/>
        </Canvas>
    </Grid>
</Page>
